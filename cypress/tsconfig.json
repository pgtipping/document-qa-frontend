{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es5", "dom"],
    "types": ["cypress", "node", "@testing-library/cypress"],
    "skipLibCheck": true,
    "strict": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "moduleResolution": "node"
  },
  "include": ["**/*.ts"],
  "exclude": [
    "node_modules",
    "../**/__tests__/**",
    "../node_modules/@types/jest"
  ]
}
